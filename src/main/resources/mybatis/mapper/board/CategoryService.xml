<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nooblol.board.mapper.UserSignOutMapper">

  <select id="selectCategory" parameterType="int" resultType="CategoryDto">
    SELECT category_id,
           category_name,
           status,
           created_user_id,
           created_at,
           updated_user_id,
           updated_at
    FROM bbs_category
    WHERE status = #{status}
  </select>

</mapper>